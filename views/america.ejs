<!doctype html>
<html lang="en">
<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <title>Ocean Wallpaper</title>
</head>
<body class="container">
<% include nav %>

<h1><%=name%></h1>
<h1><%=tuoi%></h1>
<h1>
  <% for(var i =0;i <array.length;i++){ %>
  <div class="btn btn-primary"> <%=array[i]%></div>
 <%} %>
</h1>
<h2><%= sinhVien.name%></h2>
<h2><%= sinhVien.sdt%></h2>
<% for(var i=0;i< danhSach.length;i++){%>
  <div class="btn btn-primary"> <%=danhSach[i].id%></div>
  <div class="btn btn-primary"> <%=danhSach[i].name%></div>
  <div>===================================================</div>
<%}%>
<h3><%=thongTin.name%></h3>
<h3><%=thongTin.sdt%></h3>
<% for(var i=0;i< thongTin.danhSach.length;i++){%>
<div class="btn btn-primary"> <%=thongTin.danhSach[i].id%></div>
<div class="btn btn-primary"> <%=thongTin.danhSach[i].name%></div>
<div>===================================================</div>
<%}%>
<h1><%= title %></h1>
<div class="lightbox">
  <div class="row">
    <div class="col-lg-6">
      <img
              src="https://cdn3.ivivu.com/2014/04/18-bai-bien-chau-my-la-tinh-ivivu.jpg"
              data-mdb-img="https://mdbcdn.b-cdn.net/img/Photos/Slides/1.webp"
              alt="Table Full of Spices"
              class="w-100 mb-2 mb-md-4 shadow-1-strong rounded"
      />
      <img
              src="https://cdn3.ivivu.com/2014/04/18-bai-bien-chau-my-la-tinh-ivivu4.jpg"
              data-mdb-img="https://mdbcdn.b-cdn.net/img/Photos/Square/1.webp"
              alt="Coconut with Strawberries"
              class="w-100 shadow-1-strong rounded"
      />
    </div>
    <div class="col-lg-6">
      <img
              src="https://cdn3.ivivu.com/2014/04/18-bai-bien-chau-my-la-tinh-ivivu8.jpg"
              data-mdb-img="https://mdbcdn.b-cdn.net/img/Photos/Slides/1.webp"
              alt="Table Full of Spices"
              class="w-100 mb-2 mb-md-4 shadow-1-strong rounded"
      />
      <img
              src="https://cdn3.ivivu.com/2014/04/18-bai-bien-chau-my-la-tinh-ivivu10.jpg"
              data-mdb-img="https://mdbcdn.b-cdn.net/img/Photos/Square/1.webp"
              alt="Coconut with Strawberries"
              class="w-100 shadow-1-strong rounded"
      />
    </div>
  </div>
</div>
